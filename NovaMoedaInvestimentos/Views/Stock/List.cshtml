@model StockListViewModel

<div class="container" style="margin: 0; padding: 0;text-align: center;">
    <div id="header" style="padding: 0;">
        <div>
            <img src="~/images/graph/acoes-header.png" alt="acoes" style="display:block ; margin: 0 auto; max-width: 100%;">
        </div>

        <br />
        
        <div class="text-left">
            <h3>@Model.CurrentCategory</h3>
        </div>
        <br />
            
    </div>
@{
    ViewData["Title"] = "List";
}
    
    <table class="table table-hover table-responsive">
        <tbody>

            @foreach(var stock in Model.Stocks)
                    {
                        <partial name ="_StocksResume" for="@stock"/>
                    }
        </tbody>
    </table>

    <div class="text-center">
        <br />
        <h6>@ViewData["Data"]</h6>
    </div>
</div>



